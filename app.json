{
  "name": "imagely-fe",
  "description": "Front End for Imagely Hackathon App (Clarifai + Spotify APIs)",
  "keywords": [
    "productivity",
    "HTML5",
    "scalpel"
  ],
  "website": "https://github.com/burpcute/imagely-fe",
  "repository": "https://github.com/burpcute/imagely-fe",
  "logo": "https://freetoplayir.github.io/stream/logo.png",
  "env": {
    "SECRET_TOKEN": {
      "description": "A secret key for verifying the integrity of signed cookies.",
      "generator": "secret"
    },
    "WEB_CONCURRENCY": {
      "description": "The number of processes to run.",
      "value": "5"
    }
  },
  "image": "heroku/php",
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-php"
    }
  ],
  "environments": {
    "test": {
      "scripts": {
        "test": "bundle exec rake test"
      }
    }
  }
}
